{
  "name": "coreanime",
  "productName": "Core Anime",
  "version": "1.0.0",
  "description": "Free Anime, For All",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "coreanime",
    "productName": "Core Anime",
    "copyright": "Copyright Â© 2019 Charlie Cittadini",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icon.ico"
    },
    "nsis": {
      "installerIcon": "src/icon.ico",
      "uninstallerIcon": "src/icon.ico",
      "uninstallDisplayName": "Core Anime",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "Charlie Cittadini",
    "email": "chaarliecittadini@gmail.com"
  },
  "license": "MIT",
  "config": {},
  "dependencies": {
    "custom-electron-titlebar": "^3.2.6",
    "electron-forge": "^5.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-titlebar": "0.0.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^13.1.4",
    "electron-builder": "^22.10.5",
    "electron-prebuilt": "1.4.13",
    "electron-prebuilt-compile": "8.2.0"
  }
}
